<Window
    x:Class="PhotosCategorier.SubWindows.RenamePhotosWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PhotosCategorier.SubWindows"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="RenamePhotosWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
            <Menu Style="{StaticResource Menu}">
            <MenuItem Header="File" Style="{StaticResource Main}" >
                <MenuItem Header="Read a Config File" Style="{StaticResource Sub}"></MenuItem>
                <MenuItem Header="Save as Config File" Style="{StaticResource Sub}"></MenuItem>
            </MenuItem>
        </Menu>
    </Grid>
</Window>
