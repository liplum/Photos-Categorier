<Window x:Class="PhotosCategorier.Main.SetLanguageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhotosCategorier.Main"
        xmlns:loc="clr-namespace:PhotosCategorier.Properties"
        mc:Ignorable="d"
        Title="{x:Static loc:Resources.SettingLanguage}" Width="300" Height="150"
        ResizeMode="NoResize">
    <local:SettingSelect x:Name="SelectLanguage" LabelContent="{x:Static loc:Resources.SettingLanguage}" Margin="0,20,0,0"></local:SettingSelect>
</Window>
